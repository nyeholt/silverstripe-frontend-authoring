{
    "name": "symbiote/silverstripe-frontend-authoring",
    "description": "Provides frontend editing capabilities for SilverStripe pages",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "frontend",
        "wysiwyg"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Symbiote",
            "homepage": "https://www.symbiote.com.au/"
        },
        {
            "name": "Marcus Nyeholt",
            "email": "marcus@symbiote.com.au"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": "^5.7"
    },
    "scripts": {
        "phpcbf": "phpcbf src/ tests/",
        "phpcs": "phpcs src/ tests/"
    },
    "autoload": {
        "psr-4": {
            "Symbiote\\FrontendEditing\\": "src/",
            "Symbiote\\FrontendEditing\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "expose": [
          "client/script"
        ],
        "installer-name": "frontend-editing"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
